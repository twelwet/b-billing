extends layout

block content
  ul
    li SPOT
      each catItem in categories[`spot`]
        each coin in baseCoins[`spot`]
          div
            a(href=`/billing/spot/${catItem}/${coin}`)= `${catItem}-${coin}`
    li FUTURES
      ul
        li USD-M
          each catFuturesItem in categories[`futures`]
            each coinFutures in baseCoins[`futures`]
              div
                a(href=`/billing/futures/${catFuturesItem}`)= `${catFuturesItem}-${coinFutures}`
        li COIN-M
          div
            a(href=`/billing/futures-coin/f-signal/BTCUSD_PERP`)= `f-signal-BTCUSD_PERP`
          div
            a(href=`/billing/futures-coin/f-classic/TRXUSD_PERP`)= `f-classic-TRXUSD_PERP`
          div
            a(href=`/billing/futures-coin/f-classic/ETHUSD_PERP`)= `f-classic-ETHUSD_PERP`
